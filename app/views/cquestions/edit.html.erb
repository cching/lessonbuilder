<h1>Edit Question</h1>
<center><h3>for <%= @cquestion.standard_id %></h3></center><br />
	

<%= form_for([@select,@cquestion]) do |f| %>

<div class="field">
  <%= f.label :standard_id, "Standard" %>
  <%= f.collection_select :standard_id, @select.standards.all, :id, :id %>
</div>

<div class="field">
	<%= f.label :content %>
	<%= f.text_area :content, :size => "90x10" %> 
	</div><br />


  <button type="submit" class="btn">Save</button>
<% end %>