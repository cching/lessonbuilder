<% if Xvocab.where(:standard_id => standard.id).empty? %>
  No preset vocabulary exist for this standard. Continue to the next page to add them.
  <% else %>

  
  <%= f.association :xvocabs, collection: Xvocab.where(:standard_id => standard.id), as: :check_boxes, :label_method => lambda { |vocab| "#{vocab.content_english}" }, input_html: {:data => {:toggle => "checkbox"}}, label: false %>
  
  <% end %>