<h1>Listing select_links</h1>

<table>
  <tr>
    <th>Select</th>
    <th>Link</th>
    <th>Comment</th>
    <th>Link</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @select_links.each do |select_link| %>
  <tr>
    <td><%= select_link.select_id %></td>
    <td><%= select_link.link_id %></td>
    <td><%= select_link.comment %></td>
    <td><%= select_link.link %></td>
    <td><%= link_to 'Show', select_link %></td>
    <td><%= link_to 'Edit', edit_select_link_path(select_link) %></td>
    <td><%= link_to 'Destroy', select_link, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Select link', new_select_link_path %>
