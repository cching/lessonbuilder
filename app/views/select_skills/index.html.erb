<h1>Listing select_skills</h1>

<table>
  <tr>
    <th>Select</th>
    <th>Skill</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @select_skills.each do |select_skill| %>
  <tr>
    <td><%= select_skill.select_id %></td>
    <td><%= select_skill.skill_id %></td>
    <td><%= select_skill.content %></td>
    <td><%= link_to 'Show', select_skill %></td>
    <td><%= link_to 'Edit', edit_select_skill_path(select_skill) %></td>
    <td><%= link_to 'Destroy', select_skill, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

